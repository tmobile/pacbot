[
  {
    "name": "compliance",
    "image": "${image}",
    "essential": true,
    "entrypoint": ["sh","-c"],
    "command": ["./entrypoint.sh"],
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80
      }
    ],
    "memory": 1024,
    "networkMode": "awsvpc",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group}",
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "${prefix_name}"
      }
    },
    "environment": [
      {
        "name": "JAR_FILE",
        "value": "${JAR_FILE}"
      },
      {
        "name": "CONFIG_PASSWORD",
        "value": "${CONFIG_PASSWORD}"
      },
      {
        "name": "CONFIG_SERVER_URL",
        "value": "${CONFIG_SERVER_URL}"
      },
      {
        "name": "ES_CLUSTER_NAME",
        "value": "${ES_CLUSTER_NAME}"
      },
      {
        "name": "ES_HEIMDALL_HOST_NAME",
        "value": "${ES_HEIMDALL_HOST_NAME}"
      },
      {
        "name": "ES_HEIMDALL_PORT",
        "value": "${ES_HEIMDALL_PORT}"
      },
      {
        "name": "ES_HOST_NAME",
        "value": "${ES_HOST_NAME}"
      },
      {
        "name": "ES_PORT",
        "value": "${ES_PORT}"
      },
      {
        "name": "LOGGING_ES_HOST_NAME",
        "value": "${LOGGING_ES_HOST_NAME}"
      },
      {
        "name": "LOGGING_ES_PORT",
        "value": "${LOGGING_ES_PORT}"
      },
      {
        "name": "PACMAN_HOST_NAME",
        "value": "${PACMAN_HOST_NAME}"
      },
      {
        "name": "RDS_PASSWORD",
        "value": "${RDS_PASSWORD}"
      },
      {
        "name": "RDS_URL",
        "value": "${RDS_URL}"
      },
      {
        "name": "RDS_USERNAME",
        "value": "${RDS_USERNAME}"
      },
      {
        "name": "REDSHIFT_URL",
        "value": "${REDSHIFT_URL}"
      },
      {
        "name": "REDSHIFT_USER_NAME",
        "value": "${REDSHIFT_USER_NAME}"
      },
      {
        "name": "REDSHIFT_PASSWORD",
        "value": "${REDSHIFT_PASSWORD}"
      },
      {
        "name": "ES_UPDATE_HOST",
        "value": "${ES_UPDATE_HOST}"
      },
      {
        "name": "ES_UPDATE_PORT",
        "value": "${ES_UPDATE_PORT}"
      },
      {
        "name": "ES_UPDATE_CLUSTER_NAME",
        "value": "${ES_UPDATE_CLUSTER_NAME}"
      },
      {
        "name": "LDAP_DOMAIN",
        "value": "${LDAP_DOMAIN}"
      },
      {
        "name": "LDAP_BASEDN",
        "value": "${LDAP_BASEDN}"
      },
      {
        "name": "LDAP_PORT",
        "value": "${LDAP_PORT}"
      },
      {
        "name": "LDAP_RESPONSETIMEOUT",
        "value": "${LDAP_RESPONSETIMEOUT}"
      },
      {
        "name": "LDAP_CONNECTIONTIMEOUT",
        "value": "${LDAP_CONNECTIONTIMEOUT}"
      },
      {
        "name": "LDAP_HOSTLIST",
        "value": "${LDAP_HOSTLIST}"
      },
      {
        "name": "CERTIFICATE_FEATURE_ENABLED",
        "value": "${CERTIFICATE_FEATURE_ENABLED}"
      },
      {
        "name": "PATCHING_FEATURE_ENABLED",
        "value": "${PATCHING_FEATURE_ENABLED}"
      },
      {
        "name": "VULNERABILITY_FEATURE_ENABLED",
        "value": "${VULNERABILITY_FEATURE_ENABLED}"
      }
    ]
  }
]
